{
  "name": "@nhogs/nestjs-pinecone",
  "version": "0.0.1",
  "engines": {
    "node": ">=16.x"
  },
  "description": "Pinecone.io vector database module for nestjs",
  "author": "Frederic Ciminera",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nhogs/nestjs-pinecone.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand"
  },
  "keywords": [
    "pinecone",
    "vector",
    "database",
    "nest",
    "nestjs",
    "module"
  ],
  "bugs": {
    "url": "https://github.com/Nhogs/nestjs-pinecone/issues"
  },
  "homepage": "https://github.com/Nhogs/nestjs-pinecone#readme",
  "devDependencies": {
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/testing": "^8.4.4",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "axios": "^0.27.2",
    "apollo-server-express": "^3.7.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^16.4.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "svg-to-ts": "^8.6.1",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/mapped-types": "^1.0.1",
    "apollo-server-express": "^3.7.0",
    "graphql": "^16.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "lib/**/*.(t|j)s",
      "!**/index.ts"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
